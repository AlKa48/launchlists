{
  "type": "object",
  "properties": {
    "chainId": {
      "type": "number",
      "enum": [1, 3, 31337]
    },
    "saleManager": {
      "type": "string",
      "pattern": "^0x[0-9a-f]{40}$"
    },
    "branding": {
      "type": "object",
      "properties": {
        "saleName": {
          "type": "string"
        },
        "logoUri": {
          "type": "string",
          "pattern": ".svg$"
        },
        "primaryColor": {
          "type": "string",
          "pattern": "^#[0-9a-f]{6}$"
        }
      },
      "required": ["saleName", "logoUri", "primaryColor"],
      "additionalProperties": false
    },
    "documents": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "uri": {
            "type": "string",
            "pattern": ".pdf$"
          },
          "appendSignaturePageUri": {
            "type": "string"
          }
        },
        "required": ["name", "uri"],
        "additionalProperties": false
      }
    },
